generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model faction {
  id        Int       @id @default(autoincrement())
  name      String    @unique
  products  product[]
}

model product {
  id        Int     @id @default(autoincrement())
  faction   String
  charName String @map("character")
  fact      String @unique
  image     String?
  factionId Int?
  factionRel faction? @relation(fields: [factionId], references: [id])
}
